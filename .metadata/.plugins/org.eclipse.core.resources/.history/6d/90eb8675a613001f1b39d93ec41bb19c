package basics;

public class Dog extends Animal {
	
	private  String earShape ; 
	private String tailShape ;
	
	public Dog() {
		// TODO Auto-generated constructor stub
		super("Mutt", "Big", 50);
	}
	
	public static void bark() {
		System.out.println("woof");
	}
	
	public void ladrar() {
		System.out.println("grrr");
	}

}
